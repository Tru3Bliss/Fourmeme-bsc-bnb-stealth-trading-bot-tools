# ===========================================
# REQUIRED VARIABLES
# ===========================================

# Your wallet private key (keep secure!)
PRIVATE_KEY=your_private_key_here

# BSC RPC endpoint
RPC_URL=your_bsc_rpc_here

# Token contract address to trade
TOKEN_ADDRESS=0x...

# ===========================================
# CONTRACT ADDRESSES (defaults provided)
# ===========================================

# Four.meme TokenManager contract
TOKEN_MANAGER2=0x5c952063c7fc8610FFDB798152D69F0B9550762b

# Four.meme Helper3 contract
HELPER3_ADDRESS=0xF251F83e40a78868FcfA3FA4599Dad6494E46034

# PancakeSwap Router contract
PANCAKE_ROUTER_ADDRESS=0x10ED43C718714eb63d5aA57B78B54704E256024E

# Wrapped BNB contract
WBNB_ADDRESS=0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c

# StealthFund contract for anonymous funding
STEALTH_FUND_ADDRESS=your_stealth_address

# ===========================================
# VOLUME BOT CONFIGURATION
# ===========================================

# Total BNB to use for volume trading
TOTAL_BNB=18

# Minimum BNB per wallet
MIN_DEPOSIT_BNB=0.01

# Maximum BNB per wallet
MAX_DEPOSIT_BNB=0.02

# Minimum wallets per cycle
MIN_BUY_WALLETS_NUM=3

# Maximum wallets per cycle
MAX_BUY_WALLETS_NUM=5

# Minimum wallets to sell
MIN_SELL_WALLETS_NUM=2

# Maximum wallets to sell
MAX_SELL_WALLETS_NUM=4

# Min % of tokens to sell
MIN_PERCENT_SELL_AMOUNT_AFTER_BUY=50

# Max % of tokens to sell
MAX_PERCENT_SELL_AMOUNT_AFTER_BUY=80

# Number of trading cycles
CYCLE_LIMIT=3

# Min seconds between cycles
MIN_PER_CYCLE_TIME=5

# Max seconds between cycles
MAX_PER_CYCLE_TIME=15

# Min delay before buy (ms)
MIN_DELAY_BUY=2000

# Max delay before buy (ms)
MAX_DELAY_BUY=8000

# Min delay before sell (ms)
MIN_DELAY_SELL=4000

# Max delay before sell (ms)
MAX_DELAY_SELL=12000

# BNB buffer for gas fees
GAS_BUFFER_BNB=0.001

# ===========================================
# OPTIONAL FEATURES
# ===========================================

# Use StealthFund for wallet funding (true/false)
STEALTH_MODE=true

# Auto-switch to PancakeSwap after migration (true/false)
USE_PANCAKE_AFTER_MIGRATION=true
